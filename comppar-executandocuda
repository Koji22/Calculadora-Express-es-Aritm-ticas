    {
      "cell_type": "code",
      "source": [
        "%%writefile dot_product.cu\n",
        "#include <iostream>\n",
        "#include <cmath>\n",
        "#include <cuda_runtime.h>\n",
        "\n",
        "// Kernel function to calculate the dot product\n",
        "__global__\n",
        "void dotProduct(int n, float *x, float *y, float *result)\n",
        "{\n",
        "    extern __shared__ float cache[];\n",
        "\n",
        "    int index = blockIdx.x * blockDim.x + threadIdx.x;\n",
        "    int stride = blockDim.x * gridDim.x;\n",
        "\n",
        "    float temp = 0.0f;\n",
        "    for (int i = index; i < n; i += stride)\n",
        "        temp += x[i] * y[i];\n",
        "\n",
        "    // Each thread stores its result in shared memory\n",
        "    cache[threadIdx.x] = temp;\n",
        "    __syncthreads();\n",
        "\n",
        "    // Reduce within the block\n",
        "    int i = blockDim.x / 2;\n",
        "    while (i != 0)\n",
        "    {\n",
        "        if (threadIdx.x < i)\n",
        "            cache[threadIdx.x] += cache[threadIdx.x + i];\n",
        "        __syncthreads();\n",
        "        i /= 2;\n",
        "    }\n",
        "\n",
        "    // The first thread in the block stores the result\n",
        "    if (threadIdx.x == 0)\n",
        "        atomicAdd(result, cache[0]);\n",
        "}\n",
        "\n",
        "int main(void)\n",
        "{\n",
        "    int N = 1<<20; // 1 milho de elementos\n",
        "    float *x, *y;\n",
        "    float *result;\n",
        "\n",
        "    // Allocate Unified Memory  accessible from CPU or GPU\n",
        "    cudaMallocManaged(&x, N * sizeof(float));\n",
        "    cudaMallocManaged(&y, N * sizeof(float));\n",
        "    cudaMallocManaged(&result, sizeof(float));\n",
        "\n",
        "    // Initialize x and y arrays on the host\n",
        "    for (int i = 0; i < N; i++) {\n",
        "        x[i] = 1.0f;\n",
        "        y[i] = 2.0f;\n",
        "    }\n",
        "    *result = 0.0f;\n",
        "\n",
        "    // Define the number of blocks and threads per block\n",
        "    int blockSiz\n",
        "\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "-96irSTreDIz",
        "outputId": "7d58b65b-38be-4a7f-feeb-78b4b83db055"
      },
      "execution_count": 3,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
